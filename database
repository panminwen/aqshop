--会员 
create table AQSHOP_VIP_USER
(
USER_ID VARCHAR(100) NOT NULL,--用户ID
USER_NAME VARCHAR(50) NULL,--用户名称
USER_PASSWD VARCHAR(50) NULL,--用户密码
USER_EMAIL VARCHAR(50) NULL,--email
USER_MOBILE VARCHAR(50) NULL,--手机
YX_BZ   CHAR(1) NULL,--有效标志 Y :有效  N:无效
EMAIL_AUTH CHAR(1) NULL,--是否邮件认证绑定
MOBILE_AUTH CHAR(1) NULL,--是否手机认证绑定
REGISTER_DATE DATE NULL,--注册时间
IS_AGGREE  CHAR(1) NULL,--是否同意宜柏服务协议
constraint primary key PK_AQSHOP_VIP_USER_001(USER_ID)
)


--订单主表
create table AQSHOP_ORDER_GROUP
(
ORDER_UUID VARCHAR(32) NOT NULL,--订单ID
ORDER_AMOUNT NUMERIC NULL,--订单总金额
YX_BZ   CHAR(1) NULL,--有效标志 Y :有效  N:无效
ORDER_DATE DATE NULL,--注册时间
USER_ID VARCHAR(32) NOT NULL,--用户ID
constraint primary key PK_AQSHOP_ORDER_GROUP_001(ORDER_UUID)
)

--订单明细表
create table AQSHOP_ORDER
(
UUID VARCHAR(32) NOT NULL,--流水号
ORDER_UUID VARCHAR(32) NOT NULL,--订单ID
PROD_UUID VARCHAR(32) NOT NULL,--商品编号
PROD_PRICE NUMERIC NULL,--商品价格
PROD_NUM   NUMERIC NULL,--商品数量
YX_BZ   CHAR(1) NULL,--有效标志 Y :有效  N:无效
ORDER_DATE DATE NULL,--注册时间
constraint primary key PK_AQSHOP_ORDER_001(UUID)
)

--商品支付主表
create table AQSHOP_PAY_GROUP
(
PAY_UUID VARCHAR(32) NOT NULL,--支付ID
PAY_AMOUNT NUMERIC NULL,--订单总金额
ORDER_UUID VARCHAR(32) NOT NULL,--订单ID
PAYWAY_ID INT NOT NULL,--支付方式ID
YX_BZ   CHAR(1) NULL,--有效标志 Y :有效  N:无效
ORDER_DATE DATE NULL,--注册时间
UUID VARCHAR(32) NOT NULL,--用户ID
constraint primary key PK_AQSHOP_ORDER_GROUP_001(ORDER_UUID)
)

--商品支付明细
create table AQSHOP_PAY
(
UUID VARCHAR(32) NOT NULL,--流水号
PAY_UUID VARCHAR(32) NOT NULL,--支付ID
ORDER_UUID VARCHAR(32) NOT NULL,--订单ID
PROD_UUID VARCHAR(32) NOT NULL,--商品编号
PROD_PRICE NUMERIC NULL,--商品价格
PROD_NUM   NUMERIC NULL,--商品数量
YX_BZ   CHAR(1) NULL,--有效标志 Y :有效  N:无效
ORDER_DATE DATE NULL,--注册时间
UUID VARCHAR(32) NOT NULL,--用户ID
constraint primary key PK_AQSHOP_ORDER_GROUP_001(ORDER_UUID)
)


--支付方式
create table AQSHOP_PAY_WAY
(
PAYWAY_ID INT NOT NULL,--支付方式ID
PAYWAY_NAME VARCHAR(100) NULL,--支付方式
YX_BZ   CHAR(1) NULL,--有效标志 Y :有效  N:无效
constraint primary key PK_AQSHOP_PAY_WAY_001(PAYWAY_ID)
)

--地区表
create table AQSHOP_REGION
(
REGION_ID INT NULL,--地区ID

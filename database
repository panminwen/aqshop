--广告表
create table AQSHOP_AD(
ADUUID VARCHAR(32) NOT NULL,--ADID
AD_IMG_URL VARCHAR(1000) NOT NULL,--AD 图片路径
UPLOAD_DATE  DATE NULL,--上传时间
UPLORD_USER_ID VARCHAR(32) NULL,--上传用户
YX_BZ   CHAR(1) NULL,--有效标志 Y :有效  N:无效
constraint primary key PK_AQSHOP_AD_001(ADUUID)
)


--清仓秒杀商品
create table AQSHOP_QCMS_PROD
(
UUID VARCHAR(32) NOT NULL,--流水号
PROD_UUID VARCHAR(32) NOT NULL,--商品编号
PROD_NAME VARCHAR(500) NULL,--商品名称
PROD_TITLE VARCHAR(100) NULL,----清仓秒杀商品标题
PROD_TITLE_IMG_URL VARCHAR(1000) NOT NULL,--清仓秒杀商品标题图片路径
PROD_TITLE_DESC VARCHAR(4000)  NULL,--清仓秒杀商品标题描述
PROD_PRICE NUMERIC NULL,--秒杀价格
MS_START_DATETIME DATE NULL,--秒杀开始时间
MS_END_DATETIME DATE NULL,--秒杀结束时间
YX_BZ   CHAR(1) NULL,--有效标志 Y :有效  N:无效
constraint primary key PK_AQSHOP_QCMS_PROD_001(UUID)
)



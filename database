constraint primary key PK_AQSHOP_REGION_001(PAYWAY_ID)
)

--楼盘销售排行
create table AQSHOP_HOUSE_SALES_RANK
(
UUID VARCHAR(32) NOT NULL,--流水号
HOUSE_NAME VARCHAR(50) NULL,--楼盘名称
HOUSE_RANK  INT NULL,--楼盘排名
PERIOD    VARCHAR(10) NULL,--周期 年 月 日
YEAR_MONT INT NULL,--楼盘销售年月
YX_BZ   CHAR(1) NULL,--有效标志 Y :有效  N:无效
REGION_ID INT NULL,--地区ID
REGION_NAME VARCHAR(50) NULL,--地区名称
constraint primary key PK_AQSHOP_HOUSE_SALES_RANK_001(UUID)
)


--楼盘销售情况
create table AQSHOP_HOUSE_SALES_INFO
(
UUID VARCHAR(32) NOT NULL,--流水号
HOUSE_NAME VARCHAR(50) NULL,--楼盘名称
HOUSE_RANK  INT NULL,--楼盘排名
PERIOD    VARCHAR(10) NULL,--周期 年  日
YEAR_MONT INT NULL,--楼盘销售年月
REGION_ID INT NULL,--地区ID
REGION_NAME VARCHAR(50) NULL,--地区名称
SALES_NUM INT NULL,--可售套数（套）
RG_NUM INT NULL,--认购套数（套）
CJ_NUM INT NULL,--成交套数（套）
CJ_PRICE NUMERIC NULL,--成交均价
YX_BZ   CHAR(1) NULL--有效标志 Y :有效  N:无效
constraint primary key PK_AQSHOP_HOUSE_SALES_INFO_001(UUID)
)


--商品房预售许可证查询表
create table AQSHOP_HOUSE_XKZ_INFO
(
UUID VARCHAR(32) NOT NULL,--流水号
SALES_HOUSE_COMPANY VARCHAR(50) NULL,--售房单位
XKZ_NO  INT NULL,--许可证号
HOUSE_ADDR VARCHAR(500) NULL,--房屋坐落地点
SALES_TYPE VARCHAR(10) NULL,--预售类别
PROJ_NAME VARCHAR(50) NULL,--项目名称
SALES_AREA NUMERIC NULL,--销售面积
HOUSE_USE  VARCHAR(30) NULL,--房屋用途性质
HOUSE_NUM  INT NULL,--房屋总套数
TDSYZ_NO   VARCHAR(30) NULL,--土地使用证号
RW_NUM    INT NULL,--入网套数
GHXKZ_NO VARCHAR(30) NULL,--规划许可证号
RW_AREA  NUMERIC NULL,--入网面积
TDSYQX  VARCHAR(50) NULL,--土地使用年限
LPZH   VARCHAR(30) NULL,--楼盘幢号
REGION_ID INT NULL,--地区ID
REGION_NAME VARCHAR(50) NULL,--地区名称
KPRQ    DATE NULL,--开盘日期
YX_BZ   CHAR(1) NULL--有效标志 Y :有效  N:无效
constraint primary key PK_AQSHOP_HOUSE_XKZ_INFO_001(UUID)
)

--收获地址
create table AQSHOP_RECE_ADDRESS(
UUID VARCHAR(32) NOT NULL,--流水号
USER_ID VARCHAR(100) NOT NULL,--用户ID
RECE_USER VARCHAR(30) NULL,--收货人
CELLPHONE_NO VARCHAR(11) NULL,--手机号码
REGION_ID INT NULL,--地区ID
REGION_NAME VARCHAR(50) NULL,--地区名称
ADDRESS VARCHAR(100) NULL,--详细地址
IS_DEFAULT_ADDRESS CHAR(1) NULL,--是否默认地址
constraint primary key PK_AQSHOP_RECE_ADDRESS_001(UUID)
)


--商家信息（供应商）
create table AQSHOP_Supplier(
UUID VARCHAR(32) NOT NULL,--流水号
shoper_name VARCHAR(50) NULL,--商家名称
passport_id VARCHAR(20) NULL,--身份证件号码
tel_id   VARCHAR(11) NULL,--电话号码
isentity_shop CHAR(1) NULL,--是否实体店
address  VARCHAR(300) NULL,--商铺地址
email    VARCHAR(50) NULL,--email
register_date DATE NULL,--注册时间
YX_BZ   CHAR(1) NULL--有效标志 Y :有效  N:无效
constraint primary key PK_AQSHOP_Supplier_001(UUID)
)


--商品图片表
create table AQSHOP_PROD_PIC
(
UUID VARCHAR(32) NOT NULL,--流水号
PROD_UUID VARCHAR(32) NOT NULL,--商品编号
PIC_URL  VARCHAR(300) NULL,--商品图片URL地址
YX_BZ   CHAR(1) NULL,--有效标志 Y :有效  N:无效
constraint primary key PK_AQSHOP_PROD_PIC_001(PROD_UUID)
)

PROD_TITLE_DESC VARCHAR(4000)  NULL,--推荐商品标题描述
PROD_PRICE NUMERIC NULL,--商品价格
YX_BZ   CHAR(1) NULL,--有效标志 Y :有效  N:无效
constraint primary key PK_AQSHOP_TJ_PROD_001(UUID)
)

